/**matchTable.wxss**/
.container {
  padding: 20px;
}
.title {
  font-size: 20px;
  margin-bottom: 20px;
}
.match-table {
  border: 1px solid #cccccc;
  border-radius: 5px;
}
.match-row {
  display: flex;
  justify-content: space-between;
  padding: 10px;
  border-bottom: 1px solid #eeeeee;
}
.match-row:last-child {
  border-bottom: none;
}
.player {
  font-weight: bold;
}
.vs {
  color: #007AFF;
}
